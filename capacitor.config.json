{
  "appId": "com.storytime.app",
  "appName": "StoryTime",
  "webDir": "dist",
  "bundledWebRuntime": false,
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#F7F9FC",
      "androidSplashResourceName": "splash",
      "showSpinner": false
    },
    "Keyboard": {
      "resize": "body",
      "style": "dark"
    },
    "Network": {
      "reachability": true
    }
  },
  "android": {
    "buildOptions": {
      "keystorePath": "release-keystore.jks",
      "keystorePassword": "storytime123",
      "keystoreAlias": "storytime",
      "keystoreAliasPassword": "storytime123"
    }
  }
}
